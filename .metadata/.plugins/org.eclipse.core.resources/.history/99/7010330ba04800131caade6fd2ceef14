package reflection.serDeser;

import java.util.HashSet;

public class DeSerializeTypes {
	static HashSet<Class> myAllTypesFirsts = new HashSet<Class>();
	static HashSet<Class> myAllTypesSeconds = new HashSet<Class>();

	/**
	 * @return the myAllTypesFirsts
	 */
	public static HashSet<Class> getMyAllTypesFirsts() {
		return myAllTypesFirsts;
	}

	/**
	 * @param myAllTypesFirsts
	 *            the myAllTypesFirsts to set
	 */
	public static void setMyAllTypesFirsts(HashSet<Class> myAllTypesFirsts) {
		DeSerializeTypes.myAllTypesFirsts = myAllTypesFirsts;
		System.out.println(":::::::::::"+DeSerializeTypes.myAllTypesFirsts.size());
	}

	/**
	 * @return the myAllTypesSeconds
	 */
	public static HashSet<Class> getMyAllTypesSeconds() {
		return myAllTypesSeconds;
	}

	/**
	 * @param myAllTypesSeconds
	 *            the myAllTypesSeconds to set
	 */
	public static void setMyAllTypesSeconds(HashSet<Class> myAllTypesSeconds) {
		DeSerializeTypes.myAllTypesSeconds = myAllTypesSeconds;
		System.out.println(":::::::::::"+DeSerializeTypes.myAllTypesSeconds.size());
	}

}
